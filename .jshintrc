{
  //  Enforcing options
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "immed": true,
  "indent": 4,
  "latedef": true,
  "maxcomplexity": 8,
  "maxdepth": 4,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,

  // Relaxing options
  "eqnull": true,

  // Environments
  "browser": true,
  "jquery": true,

  "globals": {

  }
}
